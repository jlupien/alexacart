# Directory for database and cookies (default: ./data/)
# Set to a Dropbox/synced folder to share across machines
DATA_DIR=

# https://cloud.browser-use.com/settings?tab=api-keys
BROWSER_USE_API_KEY=

ALEXA_LIST_NAME=Grocery List
INSTACART_STORE=Wegmans

# When debugging, you can skip checking off list items
#SKIP_ALEXA_CHECKOFF=true

# Debug: clear browser cookies on order start to test the login workflow
# Amazon cookies live in data/browser-profile-pool-0/
# Instacart cookies live in data/browser-profile/
#DEBUG_CLEAR_AMAZON_COOKIES=true
#DEBUG_CLEAR_INSTACART_COOKIES=true

# Controls concurrency during the item search phase and the add-to-cart phase
SEARCH_CONCURRENCY=8
